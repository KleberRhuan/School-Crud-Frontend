{
  "name": "houer-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "cypress open",
    "e2e:run": "cypress run",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "refactor:metrics": "node scripts/measure-refactoring-progress.cjs",
    "refactor:complexity": "npx complexity-report --output complexity-report.json --format json src/",
    "refactor:sloc": "npx sloc --format json src/ > sloc-report.json",
    "quality:check": "pnpm lint && pnpm type-check && pnpm test:run",
    "quality:full": "pnpm quality:check && pnpm refactor:metrics"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.1.1",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@sentry/react": "^9.29.0",
    "@sentry/tracing": "^7.120.3",
    "@sentry/vite-plugin": "^3.5.0",
    "@stomp/stompjs": "^7.1.1",
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-router": "^1.121.21",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.10",
    "ag-grid-community": "^33.3.2",
    "ag-grid-react": "^33.3.2",
    "axios": "^1.10.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "dexie": "^4.0.11",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.18.1",
    "immer": "^10.1.1",
    "lodash-es": "^4.17.21",
    "notistack": "^3.0.2",
    "papaparse": "^5.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "react-hook-form": "^7.58.0",
    "sockjs-client": "^1.6.1",
    "zod": "^3.25.64",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@playwright/test": "^1.53.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.10",
    "@tanstack/react-query-devtools": "^5.80.7",
    "@tanstack/react-router-devtools": "^1.121.21",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/benchmark": "^2.1.5",
    "@types/lodash-es": "^4.17.12",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/sockjs-client": "^1.5.4",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "benchmark": "^2.1.4",
    "cypress": "^14.4.1",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.2",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.12.1"
}